            <method name="beforeOpen"><![CDATA[importPackage(Packages.com.raritan.tdz.reports.eventhandler);

//attach where clause to filter certain ITEM ids
items = reportContext.getParameterValue(ReportConstants.ITEM_LIST_PARAM_ID);
if (items) {
	info("Append Item List to Query");
	itemlist = "";
	for (var i = 0; i < items.length; i++) {
		if (i > 0) itemlist += ",";
		itemlist += String(items[i]);
	}
	
	this.queryText += " WHERE ports.src_item_id IN (" + itemlist + ")";
	debug("Resulting Query " + this.queryText);
}
/*
this.queryText += " GROUP BY \
					ports.src_item_id, ports.src_item_name, src_parent_items.item_id,\
					src_parent_items.item_name, src_parent_class.lkp_value_code, \
					ports.src_port_name, grp.lku_value, ports.src_class, ports.src_class_code, \
					color.lku_value, dest_items.item_id, dest_items.item_name, dest_dc_location_details.dc_name, \
					ports.dest_port_name, dest_class.lkp_value, dest_parent_items.item_name, \
					cord.lku_value, cord.lku_attribute ";
*/					

this.queryText += " ORDER BY ports.src_port_name ASC ";
//set connection type
type = reportContext.getParameterValue(ReportConstants.CONNECTION_TYPE_PARAM_ID);
/*
	null means all -> default is true on the data set input parameter
*/
if (type != null) {
	type = type.toLowerCase();
	if (type != ReportConstants.CONNECTION_TYPE_ALL) {
		info("Evaluating Parameter Connection type :'" + type + "'");
	
		this.setInputParameterValue("enable_dataconnection0", type == ReportConstants.CONNECTION_TYPE_DATA);
		this.setInputParameterValue("enable_powerconnection0", type == ReportConstants.CONNECTION_TYPE_POWER);
		this.setInputParameterValue("enable_powerconnection1", type == ReportConstants.CONNECTION_TYPE_POWER);

	}
}

]]></method>
