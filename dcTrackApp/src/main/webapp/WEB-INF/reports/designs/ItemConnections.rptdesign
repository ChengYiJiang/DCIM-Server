            <xml-property name="queryText"><![CDATA[SELECT 	ports.src_item_id AS src_item_id,
	ports.src_item_name AS src_item_name,
	src_parent_items.item_id AS src_parent_item_id,
	src_parent_items.item_name AS src_parent_name,
	src_parent_class.lkp_value_code AS src_parent_class_code,
	ports.src_port_name AS src_port_name,
	ports.isData AS src_port_is_data,
	grp.lku_value AS src_port_group,
	ports.src_class AS src_class,
	ports.src_class_code AS src_class_code,
	color.lku_value AS src_color,
	dest_items.item_id AS dest_item_id,
	dest_items.item_name AS dest_item_name,
	dest_dc_location_details.dc_name AS dest_dc_name,
	ports.dest_port_name AS dest_port_name,
	dest_class.lkp_value AS dest_class,
	dest_parent_items.item_name AS dest_parent_name,
	cord.lku_value AS cord,
	cord.lku_attribute AS cord_color
FROM 
	( 	
	/*
	 Data
	*/
	select 	src_ports.port_name AS src_port_name,
			src_ports.port_data_id AS src_port_id,
			src_ports.vlan_lku_id AS src_port_group,
			src_ports.color_lku_id AS src_color_lku_id,
			src_items.item_id AS src_item_id,
			src_items.item_name AS src_item_name,
			src_items.parent_item_id AS src_parent_id,
			src_class.lkp_value AS src_class,
			src_class.lkp_value_code AS src_class_code,
			dest_ports.port_name AS dest_port_name,
			dest_ports.item_id AS dest_item_id,
			dest_ports.port_data_id AS dest_port_id,
			src_con.cord_id,
			true as isData
	FROM dct_connections_data as src_con

	inner join dct_ports_data as src_ports on src_con.source_port_id = src_ports.port_data_id
	inner join dct_items as src_items on src_ports.item_id = src_items.item_id
	inner join dct_lks_data as src_class on src_items.class_lks_id = src_class.lks_id

	inner join dct_circuits_data on position((',' || src_con.connection_data_id  || ',') in circuit_trace) > 0
	inner join dct_connections_data as dest_con on 
		(dct_circuits_data.start_conn_id = dest_con.connection_data_id OR 
		 dct_circuits_data.end_conn_id = dest_con.connection_data_id) AND 
		 src_con.connection_data_id <> dest_con.connection_data_id
	inner join dct_ports_data as dest_ports on dest_con.source_port_id = dest_ports.port_data_id

	WHERE ? 
	
	union
	
	/*
		Power -> device + network + probe 
	*/
	select 	src_ports.port_name AS src_port_name,
			src_ports.port_power_id AS src_port_id,
			src_ports.fuse_lku_id AS src_port_group,
			src_ports.color_lku_id AS src_color_lku_id,
			src_items.item_id AS src_item_id,
			src_items.item_name AS src_item_name,
			src_items.parent_item_id AS src_parent_id,
			src_class.lkp_value AS src_class,
			src_class.lkp_value_code AS src_class_code,
			dest_ports.port_name AS dest_port_name,
			dest_ports.item_id AS dest_item_id,
			dest_ports.port_power_id AS dest_port_id,
			src_con.cord_id,
			false as isData
	FROM dct_connections_power as src_con

	inner join dct_ports_power as src_ports on src_con.source_port_id = src_ports.port_power_id
	inner join dct_items as src_items on src_ports.item_id = src_items.item_id
	inner join dct_lks_data as src_class on src_items.class_lks_id = src_class.lks_id

	inner join dct_circuits_power on position((',' || src_con.connection_power_id  || ',') in circuit_trace) > 0
	inner join dct_connections_power as dest_con on 
		(dct_circuits_power.end_conn_id = dest_con.connection_power_id) AND 
		 src_con.connection_power_id <> dest_con.connection_power_id
	inner join dct_ports_power as dest_ports on dest_con.source_port_id = dest_ports.port_power_id

	WHERE 	src_class.lkp_value_code in (1200, 1300, 1400) /* device + network + probe  */ and ?

	union

	/*
		Power -> no device + network + probe 
	*/
	select 	src_ports.port_name AS src_port_name,
			src_ports.port_power_id AS src_port_id,
			src_ports.fuse_lku_id AS src_port_group,
			src_ports.color_lku_id AS src_color_lku_id,
			src_items.item_id AS src_item_id,
			src_items.item_name AS src_item_name,
			src_items.parent_item_id AS src_parent_id,
			src_class.lkp_value AS src_class,
			src_class.lkp_value_code AS src_class_code,
			dest_ports.port_name AS dest_port_name,
			dest_ports.item_id AS dest_item_id,
			dest_ports.port_power_id AS dest_port_id,
			src_con.cord_id,
			false as isData
	FROM dct_connections_power as src_con

	inner join dct_ports_power as src_ports on src_con.source_port_id = src_ports.port_power_id
	inner join dct_lks_data as src_ports_class on src_ports.subclass_lks_id = src_ports_class.lks_id
	inner join dct_items as src_items on src_ports.item_id = src_items.item_id
	inner join dct_lks_data as src_class on src_items.class_lks_id = src_class.lks_id

	inner join dct_circuits_power on position((',' || src_con.connection_power_id  || ',') in circuit_trace) > 0
	inner join dct_connections_power as dest_con on 
		(dct_circuits_power.start_conn_id = dest_con.connection_power_id) AND 
		 src_con.connection_power_id <> dest_con.connection_power_id
	inner join dct_ports_power as dest_ports on dest_con.source_port_id = dest_ports.port_power_id

	WHERE 	src_ports_class.lkp_value_code <> 20002 /* input */ and
			src_class.lkp_value_code not in (1200, 1300, 1400) /* device + network + probe  */ and ?
			
) AS ports
INNER JOIN dct_items AS dest_items ON ports.dest_item_id = dest_items.item_id
INNER JOIN dct_lks_data AS dest_class ON dest_items.class_lks_id = dest_class.lks_id

INNER JOIN dct_items AS src_parent_items ON ports.src_parent_id = src_parent_items.item_id
INNER JOIN dct_lks_data AS src_parent_class ON src_parent_items.class_lks_id = src_parent_class.lks_id

INNER JOIN dct_items AS dest_parent_items ON dest_items.parent_item_id = dest_parent_items.item_id
INNER JOIN dct_locations AS dest_dc_location_details ON dest_items.location_id = dest_dc_location_details.location_id

LEFT JOIN dct_lku_data AS color ON ports.src_color_lku_id = color.lku_id

LEFT JOIN dct_cords ON ports.cord_id = dct_cords.cord_id
LEFT JOIN dct_lku_data AS cord ON dct_cords.cord_lku_id = cord.lku_id
LEFT JOIN dct_lku_data AS grp ON ports.src_port_group = grp.lku_id

